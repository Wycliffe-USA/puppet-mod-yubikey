{
  "name": "ncorrare-yubikey",
  "version": "1.0.7",
  "author": "ncorrare",
  "summary": "A Puppet Module to install the yubikey PAM module and manage users/keys on a system.",
  "license": "GPL-2.0",
  "project_page": "https://github.com/ncorrare/ncorrare-yubikey/wiki",
  "source": "https://github.com/ncorrare/ncorrare-yubikey",
  "issues_url": "https://github.com/ncorrare/ncorrare-yubikey/issues",
  "tags": [
    "yubikey",
    "otp",
    "two-factor",
    "authentication",
    "pam"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04",
        "22.04"
      ]
    },
    {
      "operatingsystem": "windows",
      "operatingsystemrelease": [
        "2019",
        "2022",
        "2025",
        "10",
        "11"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": " >=2025.7.0 < 2026.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 9.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 9.0.0 < 10.0.0"
    },
    {
      "name": "puppet-epel",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "puppet/augeasproviders_core",
      "version_requirement": ">= 4.0.0 < 7.0.0"
    },
    {
      "name": "puppet/augeasproviders_pam",
      "version_requirement": ">= 5.0.0 < 6.0.0"
    }
  ]
}
